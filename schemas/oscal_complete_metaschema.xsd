               <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                  <p><b>Information Type Description</b>: A summary of how this information type is used within the system.</p>
               </xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="categorization"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="AssemblyOscalSspSystemInformationInformationTypeCategorizationType">
            <xs:annotation>
               <xs:appinfo>
                  <formal-name>Information Type Categorization</formal-name>
                  <description>A set of information type identifiers qualified by the given identification <code>system</code> used, such as NIST SP 800-60.</description>
               </xs:appinfo>
               <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
