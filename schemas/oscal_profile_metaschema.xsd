   <xs:complexType name="AssemblyOscalProfileMergeFlatType">
      <xs:annotation>
         <xs:appinfo>
            <formal-name>Flat Without Grouping</formal-name>
            <description>Directs that controls appear without any grouping structure.</description>
         </xs:appinfo>
         <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
            <p><b>Flat Without Grouping</b>: Directs that controls appear without any grouping structure.</p>
         </xs:documentation>
      </xs:annotation>
