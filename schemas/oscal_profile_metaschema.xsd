   <xs:complexType name="AssemblyOscalProfileSelectControlByIdType">
      <xs:annotation>
         <xs:appinfo>
            <formal-name>Select Control</formal-name>
            <description>Select a control or controls from an imported control set.</description>
         </xs:appinfo>
         <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
            <p><b>Select Control</b>: Select a control or controls from an imported control set.</p>
            <p>If <code>with-child-controls</code> is <q>yes</q> on the call to a control, no sibling <code>call</code>elements need to be used to call any controls appearing within it. Since generally, this is how control enhancements are represented (as controls within controls), this provides a way to include controls with all their dependent controls (enhancements) without having to call them individually.</p>
         </xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="with-id"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="TokenDatatype">
            <xs:annotation>
               <xs:appinfo>
                  <formal-name>Match Controls by Identifier</formal-name>
                  <description>Selecting a control by its ID given as a literal.</description>
               </xs:appinfo>
               <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                  <p><b>Match Controls by Identifier</b>: Selecting a control by its ID given as a literal.</p>
               </xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="matching"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="AssemblyOscalProfileMatchingType">
            <xs:annotation>
               <xs:appinfo>
                  <formal-name>Match Controls by Pattern</formal-name>
                  <description>Selecting a set of controls by matching their IDs with a wildcard pattern.</description>
               </xs:appinfo>
               <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                  <p><b>Match Controls by Pattern</b>: Selecting a set of controls by matching their IDs with a wildcard pattern.</p>
               </xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute name="with-child-controls">
         <xs:annotation>
            <xs:appinfo>
               <formal-name>Include Contained Controls with Control</formal-name>
               <description>When a control is included, whether its child (dependent) controls are also included.</description>
            </xs:appinfo>
            <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
               <p><b>Include Contained Controls with Control</b>: When a control is included, whether its child (dependent) controls are also included.</p>
            </xs:documentation>
         </xs:annotation>
         <xs:simpleType>
            <xs:restriction base="TokenDatatype">
               <xs:enumeration value="yes">
                  <xs:annotation>
                     <xs:documentation>
                        <p>Include child controls with an included control.</p>
                     </xs:documentation>
                  </xs:annotation>
               </xs:enumeration>
               <xs:enumeration value="no">
                  <xs:annotation>
                     <xs:documentation>
                        <p>When importing a control, only include child controls that are also explicitly called.</p>
                     </xs:documentation>
                  </xs:annotation>
               </xs:enumeration>
            </xs:restriction>
         </xs:simpleType>
